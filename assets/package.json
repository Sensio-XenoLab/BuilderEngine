        "main": "controllers/navigation/xenolab_xeno_engine_bundle_builder_controller.js",
        "fetch": "eager",
        "enabled": true
      },
      "xenolab_xeno_engine_bundle_navigation_entities": {
        "main": "controllers/navigation/xenolab_xeno_engine_bundle_entities_controller.js",
        "fetch": "eager",
        "enabled": true
      },
      "xenolab_xeno_engine_bundle_modules": {
        "main": "controllers/xenolab_xeno_engine_bundle_modules_controller.js",
